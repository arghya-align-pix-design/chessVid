{
  "files": [],
  "references": [
    { "path": "./tsconfig.app.json" },
    { "path": "./tsconfig.node.json" }
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "target": "ESNext",
    "module": "ESNext",
    "jsx": "react-jsx",
    "strict": true,
    "moduleResolution": "Node",
    "skipLibCheck": true,
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    //"moduleResolution": "node",
    "esModuleInterop": true
  },
  "include": ["src"]
}
